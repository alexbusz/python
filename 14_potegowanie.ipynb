    "def potega(N, P):\n",
    "    \n",
    "    if P == 0:\n",
    "        return 1\n",
    "    \n",
    "    return (N*potega(N, P-1)) \n",
    "\n",
    "N = 2\n",
    "P = 3\n",
    " \n",
    "print(potega(N, P))"
   ]
